{
    "rules": {
      "strict": 0,
      "no-underscore-dangle": 0,
      "quotes": [
        2,
        "single"
      ],
      "no-console": 0
    },
    "env": {
        "es6": true,
        "node": true,
        "browser": true,
        "mocha": true
    },
    "extends": "eslint:recommended",
    "ecmaFeatures": {
        "jsx": true,
        "experimentalObjectRestSpread": true,
        "modules": true
    },
    "plugins": [
        "react"
    ]
}
